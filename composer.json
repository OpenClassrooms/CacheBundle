{
    "name": "openclassrooms/cache-bundle",
    "description": "Symfony2 Bundle for OpenClassrooms Cache ",
    "type": "symfony-bundle",
    "target-dir": "OpenClassrooms/Bundle/OpenClassroomsCacheBundle",
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
        {
            "name": "OpenClassrooms",
            "email": "technique@openclassrooms.com"
        },
        {
            "name": "Romain Kuzniak",
            "email": "romain.kuzniak@openclassrooms.com"
        }
    ],
    "autoload": {
        "psr-0": {
            "OpenClassrooms\\Bundle\\CacheBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-0": {
            "OpenClassrooms\\Tests": "vendor/openclassrooms/cache/tests/"
        }
    },
    "require": {
        "php": ">=5.3.3",
        "openclassrooms/cache": "1.0.*@dev",
        "symfony/dependency-injection": "~2.1",
        "symfony/config": "~2.1",
        "symfony/http-kernel": "~2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~3.7"
    }
}
